    "bugs": {
        "url": "https://github.com/Chema22R/printers-discovery/issues"
    },
    "homepage": "https://github.com/Chema22R/printers-discovery#readme",
    "dependencies": {
        "@sentry/node": "^5.13.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "logdna": "^3.5.0",
        "mongodb": "^3.5",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
